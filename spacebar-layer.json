{
  "description": "Spacebar layer: Navigation keys (arrows, word, select, home/end, pgup/dn)",
  "manipulators": [
    {
      "type": "basic",
      "from": { 
        "key_code": "spacebar",
        "modifiers": { "optional": ["any"] }
      },
      "to_if_alone": [
        {
          "set_variable": {
            "name": "spacebar_modifier",
            "value": 0
          }
        },
        { "key_code": "spacebar", "halt": true }
      ],
      "to": [
        {
          "set_variable": {
            "name": "spacebar_modifier",
            "value": 1
          }
        }
      ],
      "to_after_key_up": [
        {
          "set_variable": {
            "name": "spacebar_modifier",
            "value": 0
          }
        }
      ],
      "to_delayed_action": {
        "to_if_canceled": [
          {
            "key_code": "spacebar",
            "modifiers": ["any"]
          }
        ]
      },
      "parameters": {
        "basic.to_delayed_action_delay_milliseconds": 100
      }
    },
    {
      "from": {
        "key_code": "k"
      },
      "to_if_held_down": [
        {
          "key_code": "left_arrow"
        }
      ],
      "to_after_key_up": [
        {
          "key_code": "left_arrow",
          "conditions": [
            {
              "type": "variable_if",
              "name": "spacebar_modifier",
              "value": 1
            }
          ]
        }
      ],
      "conditions": [
        {
          "type": "variable_if",
